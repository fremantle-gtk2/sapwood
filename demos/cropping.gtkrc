# vim:set ft=gtkrc sw=2 sts=2 expandtab:
pixmap_path "images"

style "button" {
  engine "sapwood" {
    image {
      function = BOX
      file = "cropping.png"
      border   = { 99, 99, 99, 99 }
    }
  }
}

class "GtkButton" style "button"

style "togglebutton" {
  engine "sapwood" {
    image {
      function = BOX
      file = "cropping-label.png"
      border   = { 99, 99, 99, 99 }
    }
  }
}

class "GtkToggleButton" style "togglebutton"
